{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noImplicitAny": false
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
